{
  "name": "angular-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:server": "./node_modules/mocha/bin/mocha --require babel-register ./server/**/*.spec.ts --exit",
    "db:up:local:all": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e local up:all",
    "db:down:local": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e local down",
    "db:local:reset": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e local reset",
    "db:up:prod:all": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e prod up:all",
    "db:down:prod": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e prod down",
    "db:prod:reset": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e prod reset",
    "db:up:circle:all": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e circle up:all",
    "db:up:travis:all": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e travis up:all",
    "serve:ssr": "node dist/server.js",
    "build:ssr": "npm run build:browser-and-server-bundles && npm run webpack:server",
    "webpack:server": "webpack --config server/webpack.server.config.js --progress --colors",
    "build:browser-and-server-bundles": "ng build --extractCss && ng run angular-website:server",
    "serve:ssr:dev": "node dist/server.js",
    "build:ssr:dev": "npm run build:browser-and-server-bundles-dev && npm run webpack:server:dev",
    "webpack:server:dev": "webpack --config server/webpack.server.config.dev.js --progress --colors",
    "build:browser-and-server-bundles-dev": "ng build --configuration=dev && ng run angular-website:server:dev",
    "serve:ssr:production": "node dist/server.js",
    "build:ssr:production": "npm run build:browser-and-server-bundles-production && npm run webpack:server:production",
    "webpack:server:production": "webpack --config server/webpack.server.config.prod.js --progress --colors",
    "build:browser-and-server-bundles-production": "ng build --prod --aot --optimization --buildOptimizer && ng run angular-website:server:production",
    "pugpostinstall": "node ./browser/ng-add-pug-loader.js",
    "changelog": "./node_modules/.bin/gulp release",
    "setup": "npm install && npm run pugpostinstall"
  },
  "pre-commit": [
    "ng lint",
    "ng test"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.14",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "^7.2.14",
    "@angular/compiler": "^7.2.14",
    "@angular/core": "^7.2.14",
    "@angular/forms": "^7.2.14",
    "@angular/http": "^7.2.14",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.14",
    "@angular/platform-browser-dynamic": "^7.2.14",
    "@angular/platform-server": "^7.2.14",
    "@angular/pwa": "^0.12.2",
    "@angular/router": "^7.2.14",
    "@angular/service-worker": "^7.2.14",
    "@nguniversal/common": "^7.1.1",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "body-parser": "^1.19.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "db-migrate": "^0.11.5",
    "db-migrate-mysql": "^1.1.10",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "mysql": "^2.17.1",
    "ng-cli-pug-loader": "^0.1.7",
    "rxjs": "~6.3.3",
    "ts-loader": "^5.4.4",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.1",
    "webpack-cli": "^3.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "~7.2.2",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/language-service": "^7.2.14",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/mysql": "^2.15.6",
    "@types/node": "~8.9.4",
    "apply-loader": "^2.0.0",
    "babel-register": "^6.26.0",
    "codelyzer": "~4.5.0",
    "dotenv": "^7.0.0",
    "gulp": "^4.0.1",
    "gulp-cli": "^2.2.0",
    "gulp-conventional-changelog": "^2.0.15",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^5.2.0",
    "protractor": "~5.4.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "string-replace-loader": "^2.1.1",
    "supertest": "^3.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.2.4"
  }
}
