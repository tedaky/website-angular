{
  "name": "angular-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:server": "./node_modules/mocha/bin/mocha --require babel-register ./server/**/*.spec.ts --exit",
    "db:up:local:all": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e local up:all",
    "db:down:local": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e local down",
    "db:local:reset": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e local reset",
    "db:up:prod:all": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e prod up:all",
    "db:down:prod": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e prod down",
    "db:prod:reset": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e prod reset",
    "db:up:circle:all": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e circle up:all",
    "db:up:travis:all": "./node_modules/db-migrate/bin/db-migrate --config ./config/database.json -e travis up:all",
    "serve:ssr": "node dist/local/server.js",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "webpack:server": "webpack --config server/webpack.server.config.js --progress --colors",
    "build:client-and-server-bundles": "ng build --extractCss && ng run angular-website:server",
    "serve:ssr:dev": "node dist/dev/server.js",
    "build:ssr:dev": "npm run build:client-and-server-bundles-dev && npm run webpack:server:dev",
    "webpack:server:dev": "webpack --config server/webpack.server.config.dev.js --progress --colors",
    "build:client-and-server-bundles-dev": "ng build --configuration=dev && ng run angular-website:server:dev",
    "serve:ssr:production": "node dist/prod/server.js",
    "build:ssr:production": "npm run build:client-and-server-bundles-production && npm run webpack:server:production",
    "webpack:server:production": "webpack --config server/webpack.server.config.prod.js --progress --colors",
    "build:client-and-server-bundles-production": "ng build --prod --aot --optimization --buildOptimizer && ng run angular-website:server:production"
  },
  "pre-commit": [
    "ng lint",
    "ng test"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "^7.2.1",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "^7.2.1",
    "@angular/pwa": "^0.12.2",
    "@angular/router": "~7.2.0",
    "@angular/service-worker": "~7.2.0",
    "@nguniversal/common": "^7.1.0",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "@types/mysql": "^2.15.5",
    "body-parser": "^1.18.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "db-migrate": "^0.11.5",
    "db-migrate-mysql": "^1.1.10",
    "express": "^4.16.4",
    "mysql": "^2.16.0",
    "rxjs": "~6.3.3",
    "ts-loader": "^5.3.3",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.1",
    "webpack-cli": "^3.2.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.2",
    "@angular/compiler-cli": "^7.2.5",
    "@angular/language-service": "~7.2.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-register": "^6.26.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^5.2.0",
    "protractor": "~5.4.0",
    "string-replace-loader": "^2.1.1",
    "supertest": "^3.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
